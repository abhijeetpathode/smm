#include <iostream>
using namespace std;

class Example {
    private:
        int value;

    public:
        // Default constructor
        Example() {
            value = 10;
            cout << "Default constructor called. Value initialized to " << value << endl;
        }

        void display() {
            cout << "Value: " << value << endl;
        }
};

int main() {
    Example obj;  // Default constructor is called
    obj.display();

    return 0;
}